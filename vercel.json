{
    "buildCommand" : "pnpm build",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/app"
        }
    ],
    "builds": [
        {
            "src": "app/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "/",
            "use": "@vercel/node"
        }
    ]
}