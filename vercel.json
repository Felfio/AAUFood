{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/app"
        }
    ],
    "builds": [
        {
            "src": "app/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "/",
            "use": "@vercel/static"
        }
    ]
}