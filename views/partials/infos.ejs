<!-- Parameters: restaurant, menuState -->
<% switch (menuState) {
    case menuStateHelper.MenuState.OnBreak: %>
        <%- include('icon-infos/on-break', {restaurant}) %>
        <% break;
    case menuStateHelper.MenuState.Closed: %>
        <%- include('icon-infos/closed', {restaurant}) %>
        <% break;
    case menuStateHelper.MenuState.NoMenu: %>
        <%- include('icon-infos/no-menu', {restaurant}) %>
        <% break;
    case menuStateHelper.MenuState.Outdated: %>
        <%- include('icon-infos/outdated', {restaurant}) %>
        <% break;
    case menuStateHelper.MenuState.Error: %>
        <%- include('icon-infos/error', {restaurant}) %>
        <% break; %>
<% } %>